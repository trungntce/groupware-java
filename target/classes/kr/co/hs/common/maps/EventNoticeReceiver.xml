<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="EventNoticeReceiver">

    <insert id="insertOne" parameterType="kr.co.hs.common.dto.NoticeReceiverDTO" useGeneratedKeys="true" keyProperty="eventNoticeReceiverId" keyColumn="EVENT_NOTICE_RECEIVER_ID">
        INSERT INTO EVENT_NOTICE_RECEIVER (EVENT_NOTICE_ID, EMP_CD, STATUS, TIME, USE_YN)
        VALUES (#{eventNoticeId}, #{empCd}, #{status}, NOW(), 'Y')
    </insert>
</mapper>